  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./build/*",
    "start": "npm run clean && npm run build-tsc && PORT=30601 node build/index.js",
    "build-tsc": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "core:update": "npm uninstall @goodtechsoft/xs-micro-service && npm uninstall @goodtechsoft/xs-mongodb && npm install @goodtechsoft/xs-micro-service && npm install @goodtechsoft/xs-mongodb",
    "broker:update": "npm uninstall @goodtechsoft/med-broker && npm install @goodtechsoft/med-broker",
    "broker:create-topics": "KAFKA_HOST=127.0.0.1:9092 ts-node src/initialize.ts --create-topics",
    "build": "npm run clean && npm run build-tsc",
    "production": "node build/index.js"
  },
  "license": "ISC",
  "dependencies": {
    "@goodtechsoft/med-broker": "^0.1.76",
